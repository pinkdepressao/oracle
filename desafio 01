let idadeUsuario = prompt('Digite a sua idade: ');

let nomeUsuario = prompt('Digite a seu nome: ');

if( idadeUsuario >=18){
    alert(nomeUsuario + ' você já pode tirar a habilitação!!');

} else{
    alert(nomeUsuario + ' espere mais um pouquinho :((');

}

